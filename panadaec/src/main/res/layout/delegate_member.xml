<?xml version="1.0" encoding="utf-8"?>

<com.scwang.smartrefresh.layout.SmartRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/pull_to_refresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F21C30"
    >
    <com.scwang.smartrefresh.layout.header.ClassicsHeader
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srlAccentColor="@color/white"
        app:srlEnableLastTime="false"></com.scwang.smartrefresh.layout.header.ClassicsHeader>
    <ScrollView
        android:id="@+id/my_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_all">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_all"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_330PX"
                android:background="@drawable/member_top_bg"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/DIMEN_80PX"
                    >

                    <ImageView
                        android:id="@+id/back"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingLeft="@dimen/DIMEN_20PX"
                        android:paddingRight="@dimen/DIMEN_20PX"
                        android:src="@drawable/white_back" />

                    <ImageView
                        android:id="@+id/my_setting_img"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true"
                        android:paddingLeft="@dimen/DIMEN_20PX"
                        android:paddingRight="@dimen/DIMEN_20PX"
                        android:src="@drawable/my_setting" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/DIMEN_25PX"
                            android:gravity="center_horizontal"
                            android:text="成功提现利润"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/DIMEN_15PX"
                            android:gravity="center_horizontal"
                            android:text="0.00000"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/DIMEN_15PX"
                            android:background="@drawable/white_border"
                            android:text="明细"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/my_headview"
                            android:layout_width="@dimen/DIMEN_120PX"
                            android:layout_height="@dimen/DIMEN_120PX"
                            android:layout_gravity="center_horizontal"
                            android:src="@drawable/my_head_icon" />

                        <TextView
                            android:id="@+id/my_level"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/DIMEN_20PX"
                            android:gravity="center"
                            android:text="初级店长"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/DIMEN_25PX"
                            android:gravity="center_horizontal"
                            android:text="可提现利润"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/DIMEN_15PX"
                            android:gravity="center_horizontal"
                            android:text="0.00000"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/DIMEN_15PX"
                            android:background="@drawable/white_border"
                            android:text="提现"
                            android:textColor="@color/white"
                            android:textSize="@dimen/DIMEN_25PX" />
                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="远方无法融化的孤傲的星空"
                    android:textColor="@color/white"
                    android:textSize="@dimen/DIMEN_25PX" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@color/white"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="我的公社"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/white"
                android:weightSum="3">

                <TextView
                    android:id="@+id/member_mycommune"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip01"
                    android:gravity="center"
                    android:paddingBottom="@dimen/DIMEN_20PX"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="我的公社"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:background="@color/list_line" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip02"
                    android:gravity="center"
                    android:paddingBottom="@dimen/DIMEN_20PX"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="海报推广"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/DIMEN_20PX"
                    android:background="@color/list_line" />

                <TextView
                    android:id="@+id/member_select"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip03"
                    android:gravity="center"
                    android:paddingBottom="@dimen/DIMEN_20PX"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="自选商品"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/DIMEN_20PX"
                android:layout_marginRight="@dimen/DIMEN_20PX"
                android:background="@color/list_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:id="@+id/member_myclient"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip04"
                    android:gravity="center"
                    android:paddingBottom="@dimen/DIMEN_20PX"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="我的客户"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/list_line" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip07"
                    android:gravity="center"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="合伙社长"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/list_line" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:drawablePadding="@dimen/DIMEN_25PX"
                    android:drawableTop="@drawable/vip06"
                    android:gravity="center"
                    android:paddingBottom="@dimen/DIMEN_20PX"
                    android:paddingTop="@dimen/DIMEN_25PX"
                    android:text="区域代理"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/DIMEN_20PX"
                android:layout_marginRight="@dimen/DIMEN_20PX"
                android:background="@color/list_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/white"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/member_profit"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/DIMEN_10PX"
                        android:drawablePadding="@dimen/DIMEN_20PX"
                        android:drawableTop="@drawable/vip05"
                        android:gravity="center"
                        android:paddingTop="@dimen/DIMEN_20PX"
                        android:text="销售利润"
                        android:textSize="@dimen/DIMEN_25PX" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal"
                        android:text="0笔"
                        android:textSize="@dimen/DIMEN_25PX" />
                </LinearLayout>

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/DIMEN_20PX"
                    android:background="@color/list_line" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/DIMEN_5PX"
                        android:drawablePadding="@dimen/DIMEN_20PX"
                        android:drawableTop="@drawable/vip09"
                        android:gravity="center"
                        android:paddingTop="@dimen/DIMEN_20PX"
                        android:text="待销售订单"
                        android:textSize="@dimen/DIMEN_25PX" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="0元"
                        android:textSize="@dimen/DIMEN_25PX" />
                </LinearLayout>

                <TextView
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/DIMEN_20PX"
                    android:background="@color/list_line" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/click"
                    android:clickable="true"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawablePadding="@dimen/DIMEN_20PX"
                        android:drawableTop="@drawable/vip08"
                        android:gravity="center"
                        android:paddingTop="@dimen/DIMEN_20PX"
                        android:text="提现明细"
                        android:textSize="@dimen/DIMEN_25PX" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="0笔"
                        android:textSize="@dimen/DIMEN_25PX" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/grey_f5" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <LinearLayout
                android:id="@+id/my_integralsgin"
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@drawable/click"
                android:clickable="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawableLeft="@drawable/vip_icon1"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="市场代理"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:drawableRight="@drawable/my_back"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@drawable/click"
                android:clickable="true"
                android:orientation="horizontal"
                android:textSize="@dimen/DIMEN_25PX">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawableLeft="@drawable/vip_icon2"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="招商代表"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:drawableRight="@drawable/my_back"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@drawable/click"
                android:clickable="true"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawableLeft="@drawable/vip_icon3"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="实体门店"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:drawableRight="@drawable/my_back"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@drawable/click"
                android:clickable="true"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawableLeft="@drawable/vip_icon4"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="商家入驻"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:drawableRight="@drawable/my_back"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/list_line" />

            <LinearLayout
                android:id="@+id/my_foot"
                android:layout_width="match_parent"
                android:layout_height="@dimen/DIMEN_80PX"
                android:background="@drawable/click"
                android:clickable="true"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/DIMEN_20PX"
                    android:drawableLeft="@drawable/vip_icon5"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:gravity="center"
                    android:text="商家管理"
                    android:textColor="@color/black_333"
                    android:textSize="@dimen/DIMEN_25PX" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/DIMEN_20PX"
                    android:drawablePadding="@dimen/DIMEN_15PX"
                    android:drawableRight="@drawable/my_back"
                    android:gravity="center"
                    android:text="查看"
                    android:textSize="@dimen/DIMEN_25PX" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</com.scwang.smartrefresh.layout.SmartRefreshLayout>
